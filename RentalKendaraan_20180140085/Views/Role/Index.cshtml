@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>
@{
    ViewData["Title"] = "Index";
}

<h2>List of Role</h2>
<a asp-action="Create">Create</a>

<table class="table-striped table-bordered">
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
        <tr>
            <td>@role.Id</td>
            <td>@role.Name</td>
        </tr>
        }
    </tbody>
</table>